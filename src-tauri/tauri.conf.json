{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "test-tauri",
  "version": "0.1.0",
  "identifier": "com.luqueee.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3007",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },

  "app": {
    "windows": [
      {
        "title": "test-tauri",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },

  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["something", "my-app"]
      }
    }
  },

  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
